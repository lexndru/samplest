{
    "request": {
        "route": "/api/v1/products/:product",
        "method": "get"
    },
    "response": {
        "code": 200,
        "data": {
            "id": "{route.product}",
            "name": "{{commerce.productName}}",
            "price": "{{commerce.price}}",
            "description": "{{commerce.product}}"
        },
        "$data": {
            "cast": {
                "id": "number",
                "price": "number"
            }
        }

    }
}
