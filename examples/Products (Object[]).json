{
	"request": {
		"route": "/api/v1/products",
		"method": "get"
	},
	"response": {
		"code": 200,
                "headers": {
                    "X-Powered-By": "Samplest"
                },
		"data": [
                    {
			"id": "{{random.number}}",
			"name": "{{commerce.productName}}",
			"price": "{{commerce.price}}",
			"description": "{{commerce.product}}",
			"image": "{{image.imageUrl}}",
                        "related": [
                            {
                                "id": "{{random.number}}"
                            },
                            {
                                "id": "{{random.number}}"
                            },
                            {
                                "id": "{{random.number}}"
                            }
                        ]
		    }
                ],
		"$data": {
			"repeat": "..10"
		}
	}
}
